type Link {
    id: ID! @id
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
    description: String!
    url: String!
    postedBy: User
    votes: [Vote!]!
}

type User {
    id: ID! @id
    name: String!
    email: String! @unique
    password: String!
    links: [Link!]!
}

type Vote {
    id: ID! @id 
    link: Link!
    user: User!
}